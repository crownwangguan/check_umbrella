{
  "manifest_version": 2,
  
  "name": "Umbrella Check",
  "description": "This extension will remind you to bring umbrella",
  "version": "1.1",

  "browser_action": {
   "default_icon": "umbrella.png",
   "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://www.google.com/*", "http://ipinfo.io/*", "https://query.yahooapis.com/*"],
      "js": ["jquery.js"]
    }
  ],
  "permissions": [
   "activeTab",
   "notifications",
   "geolocation"
   ]
}